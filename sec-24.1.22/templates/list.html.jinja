{% extends 'base.html.jinja' %}
{% block title %}User List{% endblock %}

{% block content %}
  <h1>User List</h1>
  <ul>
    {% for user in users %}
    <li><a href="/users/{{ user.id }}">{{ user.full_name }}</a></li>
    {% endfor %}
  </ul>
  <p><a href="/users/new"><button>Add User</button></a></p>
{% endblock %}